<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
  <title>Document</title>
</head>

<body>
  <%- include('./navigation.ejs')%>
  <div id="queryResponseTable" class="table-responsive mx-auto padded bg-light p-4">
    <div class="text px-3">
      <h4>query response</h4>
    </div>
    <table class="table-light pt-3">
      <tr>
        <thead>
          <% Object.keys(fields).forEach(function(key) {%>
          <th><%-fields[key].name%></th>
          <% }); %>
        </thead>
      </tr>
      <tbody>
        <% Object.keys(result).forEach(function(key) { %>
        <tr><% Object.values(result[key]).forEach(function(value) {%>
          <td><%-value%></td>
          <% }) %>
        </tr>
        <% }) %>
      </tbody>
    </table>
    <a href="/customQuery">
      <button value="back">Back</button>
    </a>
    <script src="/scripts/jquery.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.min.js" integrity="sha384-IDwe1+LCz02ROU9k972gdyvl+AESN10+x7tBKgc9I5HFtuNz0wWnPclzo6p9vxnk" crossorigin="anonymous"></script>
</body>

</html>